<table border="1">
  <th>Users Full Name</th>
  <th>Users Email</th>
  <th>Admin?</th>
  <th>Edit</th>
  <th>Delete</th>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.full_name%></td>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
      <td><%= link_to "edit", edit_admin_user_path(user)%></td>
      <td><%= link_to "delete", admin_user_path(user), method: :delete, data: {confirm: "are you sure?"}%></td>
    </tr>
  <% end %>
</table>
<%= paginate @users %>